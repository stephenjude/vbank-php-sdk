{
    "name": "vbanksdk/vbank-php-sdk",
    "description": "PHP SDK to easily work with the VDF Microfinance Bank APIs.",
    "keywords": [
        "VBankSDK",
        "vbank-php-sdk"
    ],
    "homepage": "https://github.com/vbanksdk/vbank-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "stephenjude",
            "email": "stephenjudesuccess@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "VBankSDK\\VBank\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VBankSDK\\VBank\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
